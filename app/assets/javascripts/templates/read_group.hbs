<div class="form-container">
  {{partial 'read_groups/edit'}}

  <div class="custom-container">
    {{#each attribute in association_meta_data}}
      <div class="checkbox">
        <label>
          <input type="checkbox" {{bind-attr class="attribute.class_name"}} >
          {{attribute.button_text}} ({{attribute.count}})
        </label>
      </div>
    {{/each}}
    <button class="btn btn-default" {{action 'deleteModels'}}>Delete Selected Associations</button>
  </div>
</div>
